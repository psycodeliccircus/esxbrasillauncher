{
  "name": "esxbrasillauncher",
  "version": "0.1.0",
  "description": "Launcher para FiveM.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/psycodeliccircus/esxbrasillauncher"
  },
  "author": {
    "name": "ESXBrasil",
    "email": "esxbrasil2021@gmail.com",
    "url": "https://github.com/psycodeliccircus/esxbrasillauncher"
  },
  "bugs": {
    "url": "https://github.com/psycodeliccircus/esxbrasillauncher/issues"
  },
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "clean": "rimraf dist",
    "changelog": "auto-changelog -o README.md --hide-credit",
    "build:win": "yarn run clean && electron-builder --win",
    "icon": "rimraf assets/generated && electron-icon-maker --input=assets/gui/nui/assets/img/Logo.png --output=assets/generated",
    "release:win": "yarn run clean && yarn run changelog && electron-builder --win -p always",
    "release:linux": "yarn run clean && electron-builder --linux -p always",
    "dist:dir": "yarn run clean && electron-builder -mwl",
    "build": "yarn run clean && electron-builder --win --linux"
  },
  "dependencies": {
    "check-internet-connected": "^2.0.5",
    "electron-dl": "^3.0.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.1.2",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-updater": "^4.3.1",
    "nan": "^2.14.1",
    "node-gyp": "^6.1.0",
    "path": "^0.12.7",
    "ps-node": "^0.1.6"
  },
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "electron": "^9.4.4",
    "electron-builder": "^23.0.3",
    "electron-icon-maker": "0.0.5",
    "electron-rebuild": "^3.2.7",
    "rimraf": "^3.0.2"
  }
}
